{
  "name": "vite-template-react",
  "version": "1.2.0",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "cy:open": "cypress open",
    "em:start": "firebase emulators:start --import=./saved-data --export-on-exit",
    "em:exec": "firebase emulators:exec --import=./saved-data 'npm start'",
    "em:execui": "firebase emulators:exec --ui --import=./saved-data 'npm start'",
    "em:cy:open": "start-server-and-test em:exec http://localhost:5173 cy:open"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.35.3",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "firebase": "^10.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.34.3",
    "c8": "^8.0.1",
    "cypress": "^13.3.3",
    "jsdom": "^22.1.0",
    "start-server-and-test": "^2.0.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.3"
  }
}
